pages = {207--211},
pmid = {2565047},
publisher = {American Association for the Advancement of Science},
title = {{Allelotype of colorectal carcinomas}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/2565047 http://www.sciencemag.org/cgi/doi/10.1126/science.2565047},
volume = {244},
