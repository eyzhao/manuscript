journal = {Multiple sclerosis (Houndmills, Basingstoke, England)},
keywords = {80 and over,Adult,Aged,Biomarkers,Chronic Progressive,Demyelinating Diseases,Echo-Planar Imaging,Female,Fixatives,Formaldehyde,Humans,Male,Middle Aged,Multiple Sclerosis,Myelin Sheath,Tissue Preservation,Water,metabolism,pathology,standards},
language = {eng},
month = {dec},
