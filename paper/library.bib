author = {Monclair, Tom and Brodeur, Garrett M and Ambros, Peter F and Brisse, Herv{\'{e}} J and Cecchetto, Giovanni and Holmes, Keith and Kaneko, Michio and London, Wendy B and Matthay, Katherine K and Nuchtern, Jed G and von Schweinitz, Dietrich and Simon, Thorsten and Cohn, Susan L and Pearson, Andrew D J},
doi = {10.1200/JCO.2008.16.6876},
journal = {Journal of Clinical Oncology },
month = {jan},
