issn = {0036-8075},
journal = {Science (New York, N.Y.)},
month = {jan},
number = {6167},
pages = {189--193},
title = {{Mutational Analysis Reveals the Origin and Therapy-driven Evolution of Recurrent Glioma}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3998672/},
