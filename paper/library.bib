journal = {Clinical Cancer Research },
month = {aug},
number = {15 },
pages = {4429--4434},
title = {{Triple-Negative Breast Cancer: Clinical Features and Patterns of Recurrence}},
url = {http://clincancerres.aacrjournals.org/content/13/15/4429.abstract},
volume = {13 },
year = {2007}
}
