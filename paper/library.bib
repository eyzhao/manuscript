title = {{Integrative Analysis of Complex Cancer Genomics and Clinical Profiles Using the cBioPortal}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23550210 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4160307 http://stke.sciencemag.org/cgi/doi/10.1126/scisignal.2004088},
