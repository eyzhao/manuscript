title = {{Personalized Oncogenomics: Clinical Experience with Malignant Peritoneal Mesothelioma Using Whole Genome Sequencing}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25798586 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4370594 http://dx.plos.org/10.1371/journal.pone.0119689},
volume = {10},
